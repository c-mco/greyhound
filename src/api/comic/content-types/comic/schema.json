{
  "kind": "collectionType",
  "collectionName": "comics",
  "info": {
    "singularName": "comic",
    "pluralName": "comics",
    "displayName": "Comic",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Title": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "Description": {
      "type": "text"
    },
    "DatePublished": {
      "type": "date",
      "required": true
    },
    "Author": {
      "type": "string",
      "default": "Erica",
      "required": true
    },
    "Comic": {
      "type": "media",
      "multiple": true,
      "required": true,
      "allowedTypes": [
        "images",
        "videos",
        "audios"
      ]
    },
    "collections": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::collection.collection",
      "mappedBy": "comics"
    },
    "tags": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::tag.tag",
      "mappedBy": "comics"
    },
    "slug": {
      "type": "uid",
      "targetField": "Title",
      "required": true
    }
  }
}
